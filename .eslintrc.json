  "extends": [
    "standard",
    "eslint:recommended",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:cypress/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-path-concat": "off"
  }
}
